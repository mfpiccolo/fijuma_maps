
<h1>Listing events</h1>

<div id="map_canvas">
    <%= gmaps4rails(@json) %>
<div>
<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Longitude</th>
    <th>Latitude</th>
    <th>Gmaps</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.address %></td>
    <td><%= event.longitude %></td>
    <td><%= event.latitude %></td>
    <td><%= event.gmaps %></td>
    <td><%= link_to 'Show', event, :class => 'btn btn-primary' %></td>
    <td><%= link_to 'Edit', edit_event_path(event), :class => 'btn btn-primary' %></td>
    <td><%= link_to 'Destroy', event, method: :delete, :class => 'btn btn-primary', data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Location', new_event_path %>
